{
    "name": "hisoft/laravel-conventions",
    "description": "Engineering conventions for Laravel projects",
    "type": "library",
    "license": "MIT",
    "require": {
      "php": "^8.2"
    },
    "require-dev": {
      "orchestra/testbench": "^9.0",
      "pestphp/pest": "^2.0",
      "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
      "psr-4": {
        "Hisoft\\Conventions\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Hisoft\\Conventions\\Tests\\": "tests/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Hisoft\\Conventions\\ConventionsServiceProvider"
        ]
      }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
